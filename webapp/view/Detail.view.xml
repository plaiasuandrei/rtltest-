<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" controllerName="rtl.controller.Detail">
	<semantic:DetailPage id="page" navButtonPress="onNavBack" showNavButton="{device>/system/phone}" title="{i18n>detailTitle}" busy="{detailView>/busy}" busyIndicatorDelay="{detailView>/delay}">
		<semantic:content>
			<ObjectHeader id="detailObjectHeader" title="{RdVendorName}" number="{ path: 'Wrbtr', formatter: '.formatter.currencyValue' }" numberUnit="{Waers}" visible="{detailView>/isDetailObjectHeaderVisisble}" titleLevel="Auto" condensed="true">
				<attributes>
					<ObjectAttribute text="{Sgtxt}" active="false" id="__attribute6"/>
				</attributes>
			</ObjectHeader>
			<IconTabBar id="iconTabBar" class="sapUiResponsiveContentPadding" select="onTabSelect">
				<items>
					<IconTabFilter id="iconTabBarFilter1" icon="sap-icon://hint" tooltip="{i18n>detailIconTabBarInfo}">
						<content>
							<ObjectHeader xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" xmlns="sap.m" id="objectHeader_copy" title="Item overview">
								<attributes>
									<ObjectAttribute text="{/#ocrInvoice/Bukrs/@sap:label}: {Bukrs}" active="false" id="__attribute6_copy"/>
									<ObjectAttribute active="false" id="__attribute4" text="{/#ocrInvoice/Ebeln/@sap:label}: {Ebeln}"/>
									<ObjectAttribute text="{/#ocrInvoice/PsPosid/@sap:label}: {PsPosid}" active="false" id="__attribute7"/>
									<ObjectAttribute xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" xmlns="sap.m" text="{/#ocrInvoice/Kostl/@sap:label}: {Kostl}" active="false" id="__attribute3"/>
								</attributes>
							</ObjectHeader>
						</content>
					</IconTabFilter>
					<IconTabFilter id="iconTabBarAttach" icon="sap-icon://attachment" tooltip="{i18n>detailIconTabBarAttachments}" count="1">
						<content>
							<List noDataText="Drop list items here" id="AttachmentList" inset="true" showSeparators="None" items="{/ocrInvoiceSet}">
								<items>
									<StandardListItem type="Navigation" counter="0" title="List Item 1" description="Description text" icon="{parts : [{path : 'pdf'}], formatter : 'model.formatter.formatAttachmentIcon'}" id="AttachmentListItems" press="onAttachment" iconInset="false"/></items>
							</List>
						</content>
					</IconTabFilter>
					<IconTabFilter count="{detailView>/count}" icon="sap-icon://search" iconColor="Negative" id="searchFilter">
						<content>
							<Bar id="__bar0">
								<contentRight>
									<sap.ui.layout:HorizontalLayout xmlns:sap.ui.layout="sap.ui.layout" id="__layout1">
										<sap.ui.layout:content>
											<Text xmlns:sap.ui.layout="sap.ui.layout" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" text="Global search: " id="__text22_copy"/>
											<Switch xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" id="__switch0"/>
										</sap.ui.layout:content>
									</sap.ui.layout:HorizontalLayout>
								</contentRight>
								<contentLeft>
									<sap.ui.core:Icon xmlns:sap.ui.layout="sap.ui.layout" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" xmlns:sap.ui.core="sap.ui.core" src="sap-icon://message-information" size="2em" id="__icon0"/>
								</contentLeft>
							</Bar>
							<SearchField xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" width="100%" placeholder="Search" id="__field0"/>
							<List noDataText="Drop list items here" id="__list1" items="{ocrInvoiceLSet}" updateFinished="onUpdateFinished">
								<items>
									<CustomListItem counter="0" id="__item9" type="DetailAndActive">
										<content>
											<sap.ui.layout:VerticalLayout xmlns:sap.ui.layout="sap.ui.layout" width="100%" id="__layout0">
												<sap.ui.layout:content>
													<Text xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" maxLines="2" id="__text106" text="{/#ocrInvoice/Ebeln/@sap:label}:{Ebelp}"/>
													<Text xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" maxLines="2" id="__text95" text="{/#ocrInvoice/Hkont/@sap:label}"/>
													<Text xmlns:sap.ui.layout="sap.ui.layout" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" text="No text specified" maxLines="2" id="__text106_copy"/>
													<Text xmlns:sap.ui.layout="sap.ui.layout" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" text="No text specified" maxLines="2" id="__text106_copy2"/>
												</sap.ui.layout:content>
											</sap.ui.layout:VerticalLayout>
										</content>
									</CustomListItem>
								</items>
							</List>
						</content>
					</IconTabFilter>
					<IconTabFilter count="10" icon="sap-icon://sys-cancel-2" iconColor="Critical" id="rejectFilter">
					    <content>
					        <ComboBox selectedItem="null" value="List Item 1" xmlns:sap.ui.core="sap.ui.core" id="RejectionCodeBox" placeholder="Select rejection reason" width="100%" selectionChange="onRejectionReassign">
					            <items>
					                <sap.ui.core:ListItem text="Item is not for me" key="item1" id="__item0"/>
					                <sap.ui.core:ListItem text="Reassign item" key="item2" id="__item1"/></items>
					        </ComboBox>
					        <SearchField width="100%" placeholder="Search" id="__field2"/>
					        <List noDataText="Drop list items here" id="__list2" items="{/ocrInvoiceSet}">
					            <items>
					                <StandardListItem type="Navigation" counter="0" title="Employee1" description="Description text" icon="sap-icon://picture" id="__item3"/></items>
					        </List>
					    </content>
					</IconTabFilter>
				</items>
			</IconTabBar>
			<!--			<Table id="lineItemsList" width="auto" items="{ocrInvoiceLSet}" updateFinished="onListUpdateFinished" noDataText="{i18n>detailLineItemTableNoDataText}" busyIndicatorDelay="{detailView>/lineItemTableDelay}" class="sapUiResponsiveMargin" visible="false">
				<headerToolbar>
					<Toolbar id="lineItemsToolbar">
						<Title id="lineItemsHeader" text="{detailView>/lineItemListTitle}"/>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column>
						<Text text="{i18n>detailLineItemTableIDColumn}"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Right">
						<Text text="{i18n>detailLineItemTableUnitNumberColumn}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<ObjectIdentifier title="{Sgtxt}" text="{Buzei}"/>
							<ObjectNumber number="{          path: 'Wrbtr',          formatter: '.formatter.currencyValue'         }" unit="{Waers}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>-->
		</semantic:content>
		<semantic:sendEmailAction>
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>
		</semantic:sendEmailAction>
	<semantic:saveAction>
	    <semantic:SaveAction id="__action0"/>
	</semantic:saveAction>
	</semantic:DetailPage>
</mvc:View>